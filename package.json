{
  "name": "logi-pharma-front",
  "version": "0.0.1",
  "description": "Our product GUI",
  "main": "main.js",
  "scripts": {
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "build:css": "node-sass --output app app ; node-sass main.scss main.css",
    "watch:scss": "nodemon -e scss -x \"npm run build:css\"",
    "init:compil": "npm-run-all -p tsc build:css ",
    "run:pharma-app": "electron ./main.js",
    "prestart-common": "npm run init:compil",
    "start-common": "npm-run-all -p tsc:w watch:scss run:pharma-app",
    "start:dev": "ENV=DEV npm run start-common",
    "start:prod": "ENV=PROD npm run start-common",
    "http-server:test": "node config/test-server.js",
    "serve:test": "npm run init:compil ; npm-run-all -p tsc:w watch:scss http-server:test",
    "daemon-http-server:test": "forever start config/test-server.js",
    "protractor:local": "protractor config/e2e.protractor.conf.js",
    "pretest": "npm run init:compil",
    "test": "npm-run-all -p daemon-http-server:test protractor:local ; forever stop config/test-server.js",
    "predist": "npm run init:compil",
    "dist": "node config/create-dist.js",
    "build": "npm run dist",
    "prepackage": "del -f dist logipharma-*/ && npm run build",
    "postpackage": "del dist",
    "package": "electron-packager dist logipharma",
    "package:mac": "npm run package -- --platform=darwin --arch=x64",
    "package:windows": "npm run package -- --platform=win32 --arch=ia32"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ekougs/logi-pharma-front"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.7",
    "bootstrap-sass": "3.3.6",
    "fast-levenshtein": "^1.1.3",
    "lodash": "^4.5.0",
    "lodash-es": "^4.5.1",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "systemjs": "0.19.20",
    "zone.js": "0.5.15"
  },
  "keywords": [],
  "author": "ekougs",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ekougs/logi-pharma-front/issues"
  },
  "homepage": "https://github.com/ekougs/logi-pharma-front#readme",
  "devDependencies": {
    "babel-cli": "6.5.1",
    "browser-sync": "^2.11.1",
    "concurrently": "^1.0.0",
    "cpx": "^1.3.1",
    "del": "^2.2.0",
    "del-cli": "^0.2.0",
    "electron-packager": "^5.2.1",
    "electron-prebuilt": "^0.36.0",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "forever": "^0.15.1",
    "node-sass": "3.4.2",
    "nodemon": "1.8.1",
    "npm-run-all": "^1.5.1",
    "protractor": "^3.1.1",
    "typescript": "^1.7.5",
    "typings": "^0.6.8",
    "uglify-js2": "^2.1.11"
  }
}
